{
  "name": "@mono/backend",
  "version": "0.0.1",
  "module": "src/server.ts",
  "type": "module",
  "bin": {
    "ha": "./dist/app.js"
  },
  "scripts": {
    "prisma:generate": "prisma db push --force-reset && prisma generate",
    "start": "bun --watch src/app.ts",
    "build": "bun build  --compile ./src/app.ts  --target=bun-windows-x64    --outfile dist/app",
    "build:update": "bun build  --compile ./src/app.ts  --target=bun-windows-x64    --outfile ../build/app"
  },
  "dependencies": {
    "@fastify/static": "^8.0.3",
    "@mono/common": "0.0.1",
    "@prisma/client": "^6.6.0",
    "bun-types": "latest",
    "comment-json": "^4.2.5",
    "fastify": "^5.0.0",
    "inquirer": "^12.3.0",
    "inversify": "^6.2.1",
    "json5-writer": "^0.2.0",
    "keypress": "^0.2.1",
    "lodash": "^4.17.21",
    "prisma": "^6.6.0",
    "recast": "^0.23.9",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/keypress": "^2.0.30"
  }
}
