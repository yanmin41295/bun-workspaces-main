{
  "name": "@mono/backend",
  "version": "0.0.1",
  "module": "src/server.ts",
  "type": "module",
  "bin": {
    "ha": "./dist/app.js"
  },
  "scripts": {
    "prisma:generate": "prisma db push --force-reset && prisma generate",
    "start": "bun --watch src/app.ts",
    "build": "bun build  --compile ./src/app.ts  ./src/public/**/*.png --target=bun-windows-x64    --outfile dist/app",
    "build:update": "bun build  --compile ./src/app.ts  --target=bun-windows-x64    --outfile ../build/app"
  },
  "dependencies": {
    "@fastify/static": "^8.0.3",
    "@mono/common": "0.0.1",
    "@prisma/client": "^6.6.0",
    "bun-types": "latest",
    "fastify": "^5.0.0",
    "lodash": "^4.17.21",
    "prisma": "^6.6.0",
    "prisma-generator-class-validator": "^6.3.4",
    "prisma-generator-typescript-interfaces": "^2.1.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "rimraf": "^6.0.1"
  }
}
